#!/usr/bin/gnuplot -persist
load 'plot_style'

set key bottom center

#le pone titulo al grafico
#set title "Costo de encontrar un cero en función de la calidad del mismo para distintos valores de tolerancia"

#pone la descripccion de los ejes
set xlabel "Costo"
set ylabel "Calidad solución"

set logscale y

set output 'eps/tolerance_graf.eps'
plot [3000:10000][1.0e-20:1.0e-5]'data/tolerance.dat' using 3:4 title "Tolerancia bla" linestyle 2
	 
#pongo la salida default para mostrar en pantalla
set term pop
set output
replot
