#!/usr/bin/gnuplot -persist
load 'plot_style'

set key bottom right

#le pone titulo al grafico
#set title "My lovely title"

#pone la descripccion de los ejes
set xlabel "X"
set ylabel "Y"

set output 'eps/grafiquin.eps'
plot 'data/pol.dat' using 1:2 title "Polinomio" with lines,\
	 'data/curvUniform.dat' using 1:2 title "Uniforme" with lines,\
	 'data/curvChordLength.dat' using 1:2 title "ChordLength" with lines,\
	 'data/curvCentripetal.dat' using 1:2 title "Centripetal" with lines

#pongo la salida default para mostrar en pantalla
set term pop
set output
replot
