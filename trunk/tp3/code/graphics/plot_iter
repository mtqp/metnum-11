#!/usr/bin/gnuplot -persist
load 'plot_style'

set key bottom center

#le pone titulo al grafico
#set title "Costo de encontrar un cero en función de la calidad del mismo para distintos valores de cantidad de iteraciones"

#pone la descripccion de los ejes
set xlabel "Cantidad de iteraciones"
set ylabel "Calidad solución"

set logscale y
set logscale x

set output 'eps/iter_graf.eps'
plot 'data/iter.dat' using 1:2 title "bla iteraciones" linestyle 2
	 
#pongo la salida default para mostrar en pantalla
set term pop
set output
replot